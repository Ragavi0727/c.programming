#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a;
    int sqr,po,base;
    scanf("%f",&a);
    scanf("%d",&sqr);
    scanf("%d",&base);
    scanf("%d",&po);
    printf("%d\n",(int)floor(a));
    printf("%d\n",(int)ceil(a));
    printf("%d\n",(int)sqrt(sqr));
    printf("%d\n",(int)pow(base,po));
    return 0;
}
