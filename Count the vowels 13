#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  char arr[20];
    scanf("%s\n",&arr);
    int alphcount=0;
    for(int i=0;arr[i]!=0;i++)
    {
if(arr[i]=='a'||arr[i]=='e'||arr[i]=='i'||arr[i]=='o'||arr[i]=='u'||arr[i]=='A'||arr[i]=='E'||arr[i]=='I'||arr[i]=='O'||arr[i]=='U')
{
    alphcount++;
}
    }
    printf("count: %d",alphcount);
    return 0;
}
